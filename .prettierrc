{
	"tabWidth": 4,
	"useTabs": true,
	"singleAttributePerLine": false,
	"indentInnerHtml": true,
	"indentSize": 4,
	"arrowParens": "always",
	"jsxSingleQuote": true,
	"printWidth": 150,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"bracketSameLine": true,
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "ignore",
	"endOfLine": "lf"
}
